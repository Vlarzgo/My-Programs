    /*
    The method that returns a String in which will be in one line numbers from 1 to 30 taken in parentheses ()
     */
    public class getNumberString {

        public static void main(String[] args) {
            System.out.println(getNumberString());
        }

        public static String getNumberString() {
            String bufferNumberString = "";
            for (int i = 0; i <= 30; i++) {
                bufferNumberString = bufferNumberString + "(" + i + ")";
            }
            return bufferNumberString;
        }

    }
